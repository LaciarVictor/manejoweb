---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<section class="services">
    <div class="container">
        <div class="title text-center">
            <h2 class="title-blue">{title}</h2>
        </div>
        <div class="container">
            <div class="row">
                <slot />
            </div>
        </div>
    </div>
</section>
<!-- Services End -->